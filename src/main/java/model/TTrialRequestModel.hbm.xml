<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TTrialRequestModel" table="t_trial_request" schema="" catalog="vdomain">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar" length="200"/>
        </property>
        <property name="phoneNum">
            <column name="phoneNum" sql-type="varchar" length="100"/>
        </property>
        <property name="companyName">
            <column name="companyName" sql-type="varchar" length="500"/>
        </property>
        <property name="eMail">
            <column name="eMail" sql-type="varchar" length="200"/>
        </property>
        <property name="tCheck">
            <column name="tCheck" sql-type="varchar" length="45"/>
        </property>
        <property name="tTime">
            <column name="tTime" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>