<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TFuncIntroductionDto" table="t_func_introduction" schema="" catalog="vdomain">
        <id name="id">
            <column name="ID" sql-type="int" not-null="true"/>
        </id>
        <property name="funcName">
            <column name="FUNC_NAME" sql-type="varchar" length="70"/>
        </property>
        <property name="funcTitle">
            <column name="FUNC_TITLE" sql-type="varchar" length="70"/>
        </property>
        <property name="funcGuideUrl">
            <column name="FUNC_GUIDE_URL" sql-type="varchar" length="500"/>
        </property>
        <property name="funcIconUrl">
            <column name="FUNC_ICON_URL" sql-type="varchar" length="500"/>
        </property>
        <property name="funcParentId">
            <column name="FUNC_PARENT_ID" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>