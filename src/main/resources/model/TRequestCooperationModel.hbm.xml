<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TRequestCooperationModel" table="t_request_cooperation" schema="" catalog="vdomain">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="companyName">
            <column name="companyName" sql-type="varchar" length="100"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar" length="400"/>
        </property>
        <property name="contacter">
            <column name="contacter" sql-type="varchar" length="70"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="200"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="30"/>
        </property>
    </class>
</hibernate-mapping>